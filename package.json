{
  "name": "@samhwang/vite-react-starter-template",
  "version": "1.0.0",
  "description": "Vite + React Starter Template",
  "msw": {
    "workerDirectory": ["public"]
  },
  "type": "module",
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "prepare": "pnpm run githooks:init",
    "githooks:init": "husky",
    "msw:init": "msw init public --save",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "pnpm run test && pnpm run typecheck",
    "test:preview": "vitest-preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "ci": "biome ci ."
  },
  "dependencies": {
    "@tanstack/react-form": "^0.42.1",
    "@tanstack/react-query": "^5.66.8",
    "@tanstack/react-query-devtools": "^5.66.8",
    "@tanstack/react-router": "^1.109.0",
    "@tanstack/router-devtools": "^1.109.0",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/router-cli": "^1.109.0",
    "@tanstack/router-vite-plugin": "^1.109.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.6",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-prepush": "^3.0.2",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6",
    "vitest-preview": "^0.0.1"
  }
}
